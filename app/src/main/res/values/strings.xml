        <!-- event 2 -->
        <string name="e_2_title">TEAM TRIVIA</string>
        <string name="e_2_day">TUE</string>
        <string name="e_2_month">Sept</string>
        <string name="e_2_date">29</string>
        <string name="e_2_time">7:30 PM</string>
        <string name="e_2_venue">THE\nBASEMENT</string>
        <string name="e_2_category">Social</string>
        <string name="e_2_description">Ready for some trivia? It\'s a trick question. Challenge your wits and know-how with 2-6 friends this week. While there are 8 rounds of trivia, enjoy even more drink rounds (and fun memories) with pals, too. Prizes, trophies, and bragging rights are on the line. </string>
        <string name="e_2_more_link">http://desmoinessocialclub.org/events/team-trivia-2015-09-29/</string>
        <string name="e_2_ticket"></string>
        <string name="e_2_price">FREE</string>
        <string name="e_2_age"></string>
        <!-- event 3 -->
